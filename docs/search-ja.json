---
layout: null
---
[
  {% assign posts = site.posts | where: "lang", "ja" %}
  {% for post in posts %}
    {
      "title"       : "{{ post.title | escape }}",
      "description" : "{{ post.description }}",
      "categories"  : "{{ post.categories | array_to_sentence_string }}",
      "url"         : "{{ site.url }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
